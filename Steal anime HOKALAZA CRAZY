-- Extra Tab (Fly, Noclip, Inf Jump, Speed, Credits)
do
    local content = contentFrames[3]

    local title = Instance.new("TextLabel", content)
    title.Size = UDim2.new(1, -20, 0, 44)
    title.Position = UDim2.new(0, 10, 0, 12)
    title.BackgroundTransparency = 1
    title.Font = Enum.Font.GothamBlack
    title.Text = "Extras"
    title.TextSize = 25
    title.TextColor3 = Color3.fromRGB(110,140,250)
    title.TextXAlignment = Enum.TextXAlignment.Left

    -- Fly Button
    local flyBtn = Instance.new("TextButton", content)
    flyBtn.Size = UDim2.new(0, 130, 0, 42)
    flyBtn.Position = UDim2.new(0, 32, 0, 70)
    flyBtn.BackgroundColor3 = Color3.fromRGB(110,140,250)
    flyBtn.Text = "Enable Fly"
    flyBtn.TextColor3 = Color3.fromRGB(255,255,255)
    flyBtn.Font = Enum.Font.GothamBold
    flyBtn.TextSize = 19
    Instance.new("UICorner", flyBtn).CornerRadius = UDim.new(0, 12)

    flyBtn.MouseButton1Click:Connect(function()
        flyBtn.Text = "Loading..."
        flyBtn.BackgroundColor3 = Color3.fromRGB(140,180,250)
        local success, err = pcall(function()
            loadstring(game:HttpGet("https://rawscripts.net/raw/Universal-Script-fly-v3-44404"))()
        end)
        if not success then
            flyBtn.Text = "Fly Failed!"
            flyBtn.BackgroundColor3 = Color3.fromRGB(255,60,60)
            wait(1.5)
        end
        flyBtn.Text = "Enable Fly"
        flyBtn.BackgroundColor3 = Color3.fromRGB(110,140,250)
    end)

    -- Noclip Button
    local noclipBtn = Instance.new("TextButton", content)
    noclipBtn.Size = UDim2.new(0, 130, 0, 42)
    noclipBtn.Position = UDim2.new(0, 182, 0, 70)
    noclipBtn.BackgroundColor3 = Color3.fromRGB(110,140,250)
    noclipBtn.Text = "Toggle Noclip"
    noclipBtn.TextColor3 = Color3.fromRGB(255,255,255)
    noclipBtn.Font = Enum.Font.GothamBold
    noclipBtn.TextSize = 19
    Instance.new("UICorner", noclipBtn).CornerRadius = UDim.new(0, 12)

    -- Noclip logic
    local noclipEnabled = false
    local noclipConn = nil
    noclipBtn.MouseButton1Click:Connect(function()
        noclipEnabled = not noclipEnabled
        noclipBtn.Text = noclipEnabled and "Noclip: ON" or "Noclip: OFF"
        noclipBtn.BackgroundColor3 = noclipEnabled and Color3.fromRGB(120,255,120) or Color3.fromRGB(110,140,250)
        if noclipEnabled then
            noclipConn = game:GetService("RunService").Stepped:Connect(function()
                local character = LocalPlayer.Character
                if character then
                    for _, part in ipairs(character:GetChildren()) do
                        if part:IsA("BasePart") then
                            part.CanCollide = false
                        end
                    end
                end
            end)
        elseif noclipConn then
            noclipConn:Disconnect()
            noclipConn = nil
            local character = LocalPlayer.Character
            if character and character:FindFirstChild("HumanoidRootPart") then
                character.HumanoidRootPart.CanCollide = true
            end
        end
    end)

    -- Infinite Jump Button
    local infJumpBtn = Instance.new("TextButton", content)
    infJumpBtn.Size = UDim2.new(0, 130, 0, 42)
    infJumpBtn.Position = UDim2.new(0, 32, 0, 122)
    infJumpBtn.BackgroundColor3 = Color3.fromRGB(110,140,250)
    infJumpBtn.Text = "Toggle Inf Jump"
    infJumpBtn.TextColor3 = Color3.fromRGB(255,255,255)
    infJumpBtn.Font = Enum.Font.GothamBold
    infJumpBtn.TextSize = 19
    Instance.new("UICorner", infJumpBtn).CornerRadius = UDim.new(0, 12)

    local infJumpEnabled = false
    local infJumpConn = nil
    infJumpBtn.MouseButton1Click:Connect(function()
        infJumpEnabled = not infJumpEnabled
        infJumpBtn.Text = infJumpEnabled and "Inf Jump: ON" or "Inf Jump: OFF"
        infJumpBtn.BackgroundColor3 = infJumpEnabled and Color3.fromRGB(120,255,120) or Color3.fromRGB(110,140,250)
        if infJumpEnabled then
            infJumpConn = game:GetService("UserInputService").JumpRequest:Connect(function()
                if LocalPlayer and LocalPlayer.Character and LocalPlayer.Character:FindFirstChildOfClass("Humanoid") then
                    LocalPlayer.Character:FindFirstChildOfClass("Humanoid"):ChangeState(Enum.HumanoidStateType.Jumping)
                end
            end)
        elseif infJumpConn then
            infJumpConn:Disconnect()
            infJumpConn = nil
        end
    end)

    -- Speed Button + Slider
    local speedVal = 16
    local speedBtn = Instance.new("TextButton", content)
    speedBtn.Size = UDim2.new(0, 130, 0, 42)
    speedBtn.Position = UDim2.new(0, 182, 0, 122)
    speedBtn.BackgroundColor3 = Color3.fromRGB(110,140,250)
    speedBtn.Text = "Speed: "..speedVal
    speedBtn.TextColor3 = Color3.fromRGB(255,255,255)
    speedBtn.Font = Enum.Font.GothamBold
    speedBtn.TextSize = 19
    Instance.new("UICorner", speedBtn).CornerRadius = UDim.new(0, 12)

    local speedEnabled = false
    local speedConn = nil
    speedBtn.MouseButton1Click:Connect(function()
        speedEnabled = not speedEnabled
        speedBtn.Text = speedEnabled and "Speed: "..speedVal.." ON" or "Speed: "..speedVal.." OFF"
        speedBtn.BackgroundColor3 = speedEnabled and Color3.fromRGB(140,180,250) or Color3.fromRGB(110,140,250)
        if speedEnabled then
            local function setSpeed()
                local char = LocalPlayer.Character
                if char and char:FindFirstChildOfClass("Humanoid") then
                    char:FindFirstChildOfClass("Humanoid").WalkSpeed = speedVal
                end
            end
            setSpeed()
            speedConn = LocalPlayer.CharacterAdded:Connect(function(char)
                char:WaitForChild("Humanoid").WalkSpeed = speedVal
            end)
        elseif speedConn then
            speedConn:Disconnect()
            speedConn = nil
            local char = LocalPlayer.Character
            if char and char:FindFirstChildOfClass("Humanoid") then
                char:FindFirstChildOfClass("Humanoid").WalkSpeed = 16
            end
        end
    end)

    -- Speed Changer (+ and -)
    local plusBtn = Instance.new("TextButton", content)
    plusBtn.Size = UDim2.new(0, 36, 0, 40)
    plusBtn.Position = UDim2.new(0, 340, 0, 122)
    plusBtn.BackgroundColor3 = Color3.fromRGB(110,140,250)
    plusBtn.Text = "+"
    plusBtn.TextColor3 = Color3.fromRGB(255,255,255)
    plusBtn.Font = Enum.Font.GothamBold
    plusBtn.TextSize = 22
    Instance.new("UICorner", plusBtn).CornerRadius = UDim.new(0, 12)

    local minusBtn = Instance.new("TextButton", content)
    minusBtn.Size = UDim2.new(0, 36, 0, 40)
    minusBtn.Position = UDim2.new(0, 300, 0, 122)
    minusBtn.BackgroundColor3 = Color3.fromRGB(110,140,250)
    minusBtn.Text = "-"
    minusBtn.TextColor3 = Color3.fromRGB(255,255,255)
    minusBtn.Font = Enum.Font.GothamBold
    minusBtn.TextSize = 22
    Instance.new("UICorner", minusBtn).CornerRadius = UDim.new(0, 12)

    plusBtn.MouseButton1Click:Connect(function()
        speedVal = math.min(speedVal + 2, 100)
        speedBtn.Text = (speedEnabled and "Speed: "..speedVal.." ON") or "Speed: "..speedVal.." OFF"
        if speedEnabled then
            local char = LocalPlayer.Character
            if char and char:FindFirstChildOfClass("Humanoid") then
                char:FindFirstChildOfClass("Humanoid").WalkSpeed = speedVal
            end
        end
    end)
    minusBtn.MouseButton1Click:Connect(function()
        speedVal = math.max(speedVal - 2, 1)
        speedBtn.Text = (speedEnabled and "Speed: "..speedVal.." ON") or "Speed: "..speedVal.." OFF"
        if speedEnabled then
            local char = LocalPlayer.Character
            if char and char:FindFirstChildOfClass("Humanoid") then
                char:FindFirstChildOfClass("Humanoid").WalkSpeed = speedVal
            end
        end
    end)

    -- Footer credits
    local credits = Instance.new("TextLabel", mainFrame)
    credits.Size = UDim2.new(1, 0, 0, 38)
    credits.Position = UDim2.new(0, 0, 1, -38)
    credits.BackgroundTransparency = 1
    credits.Text = "made by CRAZY   |   thx for joining discord"
    credits.TextColor3 = Color3.fromRGB(110, 140, 250)
    credits.TextSize = 18
    credits.Font = Enum.Font.GothamBold
    credits.TextXAlignment = Enum.TextXAlignment.Center
end

-- Features Tab
do
    local content = contentFrames[4]

    local title = Instance.new("TextLabel", content)
    title.Size = UDim2.new(1, -20, 0, 44)
    title.Position = UDim2.new(0, 10, 0, 12)
    title.BackgroundTransparency = 1
    title.Font = Enum.Font.GothamBlack
    title.Text = "Features"
    title.TextSize = 25
    title.TextColor3 = Color3.fromRGB(110,140,250)
    title.TextXAlignment = Enum.TextXAlignment.Left

    local featuresText = Instance.new("TextLabel", content)
    featuresText.Size = UDim2.new(1, -40, 0, 220)
    featuresText.Position = UDim2.new(0, 20, 0, 70)
    featuresText.BackgroundColor3 = Color3.fromRGB(54, 57, 66)
    featuresText.TextColor3 = Color3.fromRGB(110,140,250)
    featuresText.Font = Enum.Font.Gotham
    featuresText.TextSize = 18
    featuresText.Text =
        "Script Features:\n" ..
        "- Teleport: Save and teleport to any position\n" ..
        "- Auto Lock: Save and teleport (for other uses)\n" ..
        "- Fly: Universal, loads external script\n" ..
        "- Noclip: Toggle collision\n" ..
        "- Inf Jump: Jump any time\n" ..
        "- Speed: Customizable walk speed\n" ..
        "- Mobile, PC, and iPad support\n" ..
        "- Rayfield-inspired GUI\n" ..
        "- Modern draggable animated GUI\n" ..
        "- Sidebar navigation\n" ..
        "- Credits at the bottom!"
    featuresText.TextWrapped = true
    featuresText.TextYAlignment = Enum.TextYAlignment.Top
    featuresText.TextXAlignment = Enum.TextXAlignment.Left
    featuresText.BackgroundTransparency = 0.06
    Instance.new("UICorner", featuresText).CornerRadius = UDim.new(0,12)
end

	else
		textbox.Text = ""
		textbox.PlaceholderText = "❌ Wrong Key! Try Again"
	end
end)

discordBtn.MouseButton1Click:Connect(function()
	setclipboard(DiscordLink)
	discordBtn.Text = "Copied!"
	task.delay(2, function()
		discordBtn.Text = "Get Key (Discord)"
	end)
end)
